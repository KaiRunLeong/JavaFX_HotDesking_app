<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.ViewDeskDetailsController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="125.0" text="Current desk situation:" />
      <Label fx:id="status" layoutX="140.0" layoutY="14.0" prefHeight="26.0" prefWidth="125.0">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <VBox layoutX="161.0" layoutY="89.0" prefHeight="230.0" prefWidth="177.0" spacing="15.0">
         <children>
            <Button fx:id="normalButton" mnemonicParsing="false" onAction="#normal" prefHeight="65.0" prefWidth="185.0" text="Normal" />
            <Button fx:id="covidConditionButton" mnemonicParsing="false" onAction="#covidCondition" prefHeight="65.0" prefWidth="185.0" text="Covid Condition" />
            <Button fx:id="lockdownButton" mnemonicParsing="false" onAction="#lockdown" prefHeight="65.0" prefWidth="185.0" text="Lockdown" />
         </children>
      </VBox>
      <Button fx:id="back" layoutX="14.0" layoutY="359.0" mnemonicParsing="false" onAction="#previousPage" text="Back" />
   </children>
</AnchorPane>

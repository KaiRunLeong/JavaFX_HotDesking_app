<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.AdminReportController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="152.0" text="Click to view report:">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <VBox layoutX="14.0" layoutY="48.0" prefHeight="200.0" prefWidth="138.0" spacing="5.0">
         <children>
            <Hyperlink fx:id="bookingReport" onAction="#bookingCSV" prefHeight="29.0" prefWidth="139.0" text="Booking CSV report" />
            <Hyperlink fx:id="employeeReport" onAction="#employeeCSV" prefHeight="29.0" prefWidth="139.0" text="Employee CSV report" />
         </children>
      </VBox>
      <Button fx:id="back" layoutX="14.0" layoutY="359.0" mnemonicParsing="false" onAction="#previousPage" text="Back" />
   </children>
</AnchorPane>

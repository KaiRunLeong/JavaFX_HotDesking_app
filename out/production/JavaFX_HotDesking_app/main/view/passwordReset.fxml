<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.PasswordResetController">
   <children>
      <Label layoutX="216.0" layoutY="14.0" text="Reset Password">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Label layoutX="173.0" layoutY="56.0" text="Username:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="173.0" layoutY="75.0" prefHeight="30.0" prefWidth="255.0" />
      <TextField fx:id="newPassword" layoutX="173.0" layoutY="131.0" prefHeight="30.0" prefWidth="255.0" />
      <Label layoutX="173.0" layoutY="112.0" text="New Password:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="173.0" layoutY="168.0" text="Re-enter password">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="reenterPass" layoutX="173.0" layoutY="185.0" prefHeight="30.0" prefWidth="255.0" />
      <Button fx:id="nextButton" layoutX="234.0" layoutY="265.0" mnemonicParsing="false" onAction="#next" prefHeight="60.0" prefWidth="135.0" text="next">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="back" layoutX="14.0" layoutY="409.0" mnemonicParsing="false" onAction="#returnToPreviousPage" text="Back" />
      <Label fx:id="invalidUser" layoutX="251.0" layoutY="57.0" prefHeight="17.0" prefWidth="101.0" textFill="#f50101" />
      <Label fx:id="passwordNotMatch" layoutX="279.0" layoutY="113.0" prefHeight="17.0" prefWidth="146.0" textFill="#fc0909" />
   </children>
</AnchorPane>
